{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": ".",
    "paths": {
      "@common/*": [
        "./src/common/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@interfaces/*": [
        "./src/interfaces/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@states/*": [
        "./src/states/*"
      ],
      "@utilities/*": [
        "./src/utilities/*"
      ]
    }
  },
  "exclude": [
    "./node_modules"
  ]
}
